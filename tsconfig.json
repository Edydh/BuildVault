{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"],
      "@/assets/*": ["assets/*"]
    }
  },
  "include": ["app", "components", "lib", "env.d.ts"],
  "exclude": ["node_modules"]
}
