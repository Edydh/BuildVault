{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "SUPABASE_URL": "public",
        "SUPABASE_ANON_KEY": "public"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "SUPABASE_URL": "public",
        "SUPABASE_ANON_KEY": "public"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production",
        "SUPABASE_URL": "public",
        "SUPABASE_ANON_KEY": "public"
      }
    }
  }
}